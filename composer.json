{
  "version": "1.0.2",
  "description": "PHP SDK for LODGEA",
  "keywords": [
    "lodgea",
    "php",
    "sdk"
  ],
  "homepage": "https://lodgea.com/",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "LODGEA",
      "homepage": "https://lodgea.com/"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^7.3",
    "guzzlehttp/psr7": "^1.7 || ^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0 || ^9.0",
    "friendsofphp/php-cs-fixer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "com.lodgea\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "com.lodgea\\Test\\": "test/"
    }
  },
  "name": "lodgea/lodgea-php",
  "type": "library"
}
